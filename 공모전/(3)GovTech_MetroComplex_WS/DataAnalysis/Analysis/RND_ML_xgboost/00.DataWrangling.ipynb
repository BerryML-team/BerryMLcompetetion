    "# Station Info 와 호선통합배차시간표 불러오기 \n",
    "def SubwayDispatchfileLoad():\n",
    "    ## 위경도 배차시간  \n",
    "    subwayInfo = pd.read_csv(\"../../Data/StationInfo.csv\")\n",
    "    latlng = pd.read_csv(\"../../Data/seoul_subway_latlon_zenzen.csv\")\n",
    "    latlng.rename( {'고유역번호(외부역코드)':'역사코드'}, inplace=True, axis=1)\n",
    "    latlng.drop(['역명','호선','환승역수'], axis=1,inplace=True)\n",
    "    subwayInfo = pd.merge(subwayInfo,latlng, on='역사코드',how='inner' )\n",
    "    # subwayInfo.tail() ## 279개 확인 \n",
    "    ## 경로에 파일이있으면 넘김, 없으면 만들고 저장 \n",
    "    file_path = \"../../Data/지하철배차시간데이터/integrated_dispatch.csv\"\n",
    "    if not os.path.exists(file_path):\n",
    "        # 호선별 라인배치 병합 \n",
    "        dfs =[]\n",
    "        line_배치_dict ={}\n",
    "        for i in range(1,9):\n",
    "            dfs.append(pd.read_csv(f'../../Data/지하철배차시간데이터/{i}호선배차.csv'))\n",
    "        dispatch_table = pd.concat(dfs,ignore_index=True)\n",
    "        dispatch_table.to_csv(\"../../Data/지하철배차시간데이터/integrated_dispatch.csv\",index=None)\n",
    "        print(yellow(\"통합 배차시간표 파일이 생성 되었습니다\"))\n",
    "    else :\n",
    "        print(yellow(\"통합 배차시간표 파일이 이미 있으므로 불러옵니다\"))\n",
    "        dispatch_table = pd.read_csv(file_path)\n",
    "    return subwayInfo,dispatch_table\n",
    "\n",
    "subwayInfo,dispatch_table=SubwayDispatchfileLoad()\n"
